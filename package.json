{
  "name": "recreation-management-api",
  "type": "module",
  "scripts": {
    "dev": "tsx --watch src/index.ts",
    "dev:wrangler": "wrangler dev --local-protocol https --ip 0.0.0.0 --port 8787",
    "deploy": "wrangler deploy --minify",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:migrate --remote": "wrangler d1 migrations apply rectime --remote",
    "db:migrate --local": "wrangler d1 migrations apply rectime --local",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.2",
    "@prisma/adapter-d1": "^5.22.0",
    "@prisma/client": "^5.22.0",
    "@types/node": "^24.5.2",
    "crypto": "^1.0.1",
    "hono": "^4.9.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250909.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "prisma": "^5.22.0",
    "tsx": "^4.4.0",
    "typescript": "^5.9.2",
    "wrangler": "^4.4.0"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
