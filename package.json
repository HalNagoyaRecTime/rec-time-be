{
  "name": "recreation-management-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx --watch src/index.local.ts",
    "dev:wrangler": "npx wrangler dev",
    "deploy": "npx wrangler deploy --minify",
    "db:migrate:local": "npx wrangler d1 migrations apply DB --local",
    "db:migrate:remote": "npx wrangler d1 migrations apply DB --remote",
    "cf:typegen": "npx wrangler types --env-interface CloudflareBindings",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint . --ext .ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.2",
    "better-sqlite3": "^12.4.1",
    "hono": "^4.9.9",
    "papaparse": "^5.5.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250924.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.5.2",
    "@types/papaparse": "^5.3.16",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsx": "^4.4.0",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "wrangler": "^4.40.2"
  },
  "packageManager": "npm@10"
}
